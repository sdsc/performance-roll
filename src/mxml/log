( \
	  if test -e /etc/profile.d/modules.sh; then . /etc/profile.d/modules.sh; module load intel; fi; \
	  export CC=icc; \
	  export CFLAGS=""; \
	  export CXX=icpc; \
	  cd mxml-2.7; \
	  ./configure -prefix=/opt/mxml/intel; \
	  make; \
	  make install \
	)
